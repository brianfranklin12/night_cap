<h1>Sign Up</h1>

<%= form_with model: @user, url: signup_path do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %> <br>
  <%= f.label :email %>
  <%= f.text_field :email %> <br>
  <%= f.label :password %>
  <%= f.password_field :password %> <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %> <br>
  <%= f.submit "Sign Up" %>
<% end %>